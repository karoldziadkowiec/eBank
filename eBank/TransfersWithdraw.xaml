        <Button x:Name="cancel_Button" Content="Cancel" HorizontalAlignment="Left" Margin="551,578,0,0" VerticalAlignment="Top" Background="White" BorderBrush="#FF003401" Foreground="#FF003401" FontFamily="Calibri" FontSize="20" Width="238" Height="40
        " Click="backToTransfersPage" TabIndex="11"/>
